<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
	<constant name="struts.multipart.saveDir" value="/d:/save" />
	
	<package name="BBuSic" extends="struts-default">
		<interceptors>
			<interceptor name="BBuSicInterceptor" class="action.BBuSicInterceptor"/>
		</interceptors>

		<action name="payInsertForm" class="action.payFormAction">
			<result>/Buy/Pay_setting.jsp</result>
		</action>
	
		<!-- 게시판 리스트 액션. -->
		<action name="payInsert" class="action.payAction">
			<interceptor-ref name="BBuSicInterceptor"/>
			<interceptor-ref name="prepare"/>
			<interceptor-ref name="modelDriven"/>
			<interceptor-ref name="params"/>
			<result>/Buy/TEST.jsp</result>
		</action>
	</package>
		
	<package name="PooSic" extends="struts-default">
		<interceptors>
			<interceptor name="input" class="member.action.MemberInterceptor" />
		</interceptors>
		
		<action name="agreement" class="member.action.AgreementAction">
			<result>/member/agreement.jsp</result>
		</action>
		<action name="inputForm" class="member.action.InputFormAction">
			<result>/member/inputForm.jsp</result>
		</action>
		<action name="confirm" class="member.action.ConfirmAction">
			<result>/member/confirm.jsp</result>
		</action>
		<action name="inputPro" class="member.action.InputProAction">
			<interceptor-ref name="input"/>
			<interceptor-ref name="prepare"/>
			<interceptor-ref name="modelDriven"/>
			<interceptor-ref name="params"/>
			<result>/member/inputPro.jsp</result>
		</action>
		
		
		<action name="loginFrom" class="member.action.LoginFormAction">
			<result>/member/loginFrom.jsp</result>
		</action>
	</package>
	
	<package name="sturts2" extends="struts-default">
		<interceptors>
			<interceptor name="fileUploadDao"
				class="upload.interceptor.FileUploadDAOInterceptor" />
		</interceptors>

		<action name="MusicUploadMain" class="upload.action.MusicUploadMainAction">
			<interceptor-ref name="prepare" />
			<interceptor-ref name="modelDriven" />
			<interceptor-ref name="params" />
			<result>/file/MusicUploadMain.jsp</result>
		</action>
		<action name="MusicUploadForm" class="upload.action.FileUploadAction">
			<interceptor-ref name="prepare" />
			<interceptor-ref name="modelDriven" />
			<interceptor-ref name="params" />
			<result>/file/MusicUploadForm.jsp</result>
		</action>
		<action name="MusicUploadPro" class="upload.action.MusicUploadProAction">
			<interceptor-ref name="prepare" />
			<interceptor-ref name="modelDriven" />
			<interceptor-ref name="fileUpload" />
			<interceptor-ref name="params" />
			<result>/file/MusicUploadPro.jsp</result>
		</action>
		<action name="ChartBoard" class="board.action.MelonChart_boardAction">
			<interceptor-ref name="prepare" />
			<interceptor-ref name="modelDriven" />
			<interceptor-ref name="params" />
			<result>/board/MelonChart_board.jsp</result>
		</action>
	</package>
	
	<package name="AAAA" extends="struts-default">
		
	</package>
	
</struts>
